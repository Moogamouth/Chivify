{
    "manifest_version": 3,
    "name": "Chivify",
    "version": "1.0",
    "description": "Viewer for Arweave archived webpages.",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "webRequest"
    ],
    "background": {
        "service_worker": "scripts/index.js"
    }
}